{
    "manifest_version": 3,
    "name": "Moodle Bulk Downloader",
    "version": "0.0.1",
    "description": "A Bulk Downloader for Moodle Assignment Submissions",
  
    "action": {
        "default_popup": "index.html"
      },
    "content_scripts": [
        {
            "matches": ["https://moodle.uni-due.de/mod/assign/*"],
            "js": ["jquery-3.6.0.min.js", "content-script.js"]
        }
    ],
    "permissions": [
      "storage"
    ]
  }
  